{
  "extends": ["react-app", "react-app/jest", "prettier"],
  "plugins": ["react-hooks"],
  "rules": {
    "semi": "warn",
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn",
    "jsx-a11y/anchor-is-valid": "off",
    "array-callback-return": "off",
    "@typescript-eslint/no-unused-vars": "warn"
  }
}
